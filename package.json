{
  "name": "solfate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "yarn contentlayer && next dev",
    "build": "yarn prisma generate && yarn contentlayer && next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "contentlayer": "contentlayer build --clearCache",
    "db:org": "pscale org switch solfate",
    "db:connect": "yarn db:org && pscale connect solfate-main --port 3309 ",
    "db:branch": "yarn db:org && pscale branch create solfate-main ",
    "db:pr": "yarn db:org && pscale deploy-request create solfate-main ",
    "db:merge": "yarn db:org && pscale deploy-request deploy ",
    "lint": "next lint"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@next/mdx": "^14.0.3",
    "@prisma/client": "^5.7.1",
    "@solana/wallet-adapter-react": "^0.15.35",
    "@solana/wallet-adapter-react-ui": "^0.9.34",
    "@solana/wallet-standard-features": "^1.1.0",
    "@solana/wallet-standard-util": "^1.1.0",
    "@solana/web3.js": "^1.87.6",
    "@vercel/analytics": "^1.1.1",
    "add": "^2.0.6",
    "bs58": "^5.0.0",
    "clsx": "^2.0.0",
    "contentlayer": "^0.3.4",
    "date-fns": "^2.30.0",
    "next": "14.0.3",
    "next-auth": "^4.24.5",
    "next-contentlayer": "^0.3.4",
    "next-mdx-remote": "^4.4.1",
    "next-sitemap": "^4.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dom-confetti": "^0.2.0",
    "react-feather": "^2.0.10",
    "react-hot-toast": "^2.4.1",
    "react-spinners": "^0.13.8"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "20.7.2",
    "@types/react": "18.2.23",
    "@types/react-dom": "18.2.8",
    "autoprefixer": "10.4.16",
    "eslint": "8.50.0",
    "eslint-config-next": "14.0.3",
    "postcss": "8.4.31",
    "prisma": "^5.7.1",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2"
  },
  "engines": {
    "node": ">=16"
  }
}
